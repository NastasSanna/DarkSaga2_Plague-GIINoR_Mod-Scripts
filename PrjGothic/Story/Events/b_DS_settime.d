// задать время, при необходимости - перевести на завтра
func void B_DS_SetTimeAfter(var int h, var int m)
{
	// если больше, чем на сутки - то как написано
	if(h >= 24)
	{
		Wld_SetTime(h, m);
	}
	// если меньше суток - проверить, не нужно или перевести на завтра
	else if(Wld_IsTime(0,0,h,m))
	{
		Wld_SetTime(h, m);
	}
	else
	{
		Wld_SetTime(h + 24, m);
	};
};

// перевод часов на h часов m минут вперед
func void B_DS_SetTimePlus(var int h, var int m)
{
	var int h_target;
	var int m_target;
	m_target = m + C_TimeMinute();
	h_target = h + C_TimeHour() + m_target / 60;
	m_target = m_target % 60;
	Wld_SetTime(h_target, m_target);	
};
